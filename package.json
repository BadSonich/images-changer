{
  "name": "simple-electron-boilerplate",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "dist:win": "npm run build && electron-builder --win --x64",
    "dist:mac": "npm run build && electron-builder --mac --x64",
    "electron": "NODE_ENV=development electron .",
    "start": "concurrently \"npm run dev\" \"npm run electron\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "build": {
    "appId": "com.example.simpleelectronboilerplate",
    "productName": "SimpleElectronBoilerplate",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "package.json"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    }
  },
  "devDependencies": {
    "@vue/cli": "^5.0.9",
    "electron": "^30.0.0",
    "electron-builder": "^26.0.12",
    "node-abi": "^4.13.0",
    "vite": "^7.1.4",
    "vite-plugin-vuetify": "^2.1.2",
    "vue": "^3.5.21",
    "vuetify": "^3.9.7"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@vitejs/plugin-vue": "^6.0.1",
    "concurrently": "^9.2.1",
    "sass": "^1.92.0",
    "vue-router": "^4.5.1"
  }
}
